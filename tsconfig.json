{
    "compilerOptions": {
        "lib": [
            "esnext",
            "dom"
        ],
        "resolveJsonModule": true,
        "moduleResolution": "node",
        "skipLibCheck": true,
        "strict": true,
        "module": "esnext",
        "target": "es6",
        "outDir": "./dist",
        "esModuleInterop": true,
        "types": ["jest"]
    },
    "include": [
        "src"
    ],
    "exclude": [
        "node_models",
        "__test__"
    ]
}
